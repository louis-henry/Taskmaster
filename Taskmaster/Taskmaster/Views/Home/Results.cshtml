<link rel="stylesheet" href="~/css/site-results.css" />
<link rel="stylesheet" href="~/css/adv-search.css" />
<div class="search-page-container">
    <div class="adv-search-container">
      <form>
        <div class="inner-form">
          <div class="basic-search">
            <div class="input-field">
              <div class="icon-wrap">
                <i class="fa fa-search"></i>
              </div>
              <input id="search" type="text" placeholder="Search..." />
              <div class="result-count">
                <a href="" id="advanced-search-button">Advanced</a></div>
            </div>
          </div>
          <div class="advance-search" id="advanced-search-area">
            <span class="desc">Advanced Search</span>
            <div class="row">
              <div class="input-field">
                <div class="input-select">
                  <select data-trigger="" name="choices-single-defaul">
                    <option placeholder="" value="">LOCATION</option>
                    <option>From Home</option>
                    <option>Requires Travel</option>
                  </select>
                </div>
              </div>
              <div class="input-field">
                <div class="input-select">
                  <select data-trigger="" name="choices-single-defaul">
                    <option placeholder="" value="">TYPE</option>
                    <option>Fixed</option>
                    <option>Tender</option>
                    <option>Bounty</option>
                  </select>
                </div>
              </div>
              <div class="input-field">
                <div class="input-select">
                  <select data-trigger="" name="choices-single-defaul">
                    <option placeholder="" value="">CATAGORY</option>
                    <option>Programming</option>
                    <option>Handyman</option>
                    <option>Design</option>
                  </select>
                </div>
              </div>
            </div>
            <div class="row second">
              <div class="input-field">
                <div class="input-select">
                  <input id="adv-s-min" onfocus="(this.type='date')" onblur="(this.type='text')" placeholder="START DATE"/>
                </div>
              </div>
              <div class="input-field">
                <div class="input-select">
                  <input id="adv-s-max" onfocus="(this.type='date')" onblur="(this.type='text')" placeholder="END DATE"/>
                </div>
              </div>
              <div class="input-field">
                <div class="input-select">
                  <input id="adv-s-pcode" type="number" min="0000" max="9999" placeholder="POSTCODE" pattern="/^(?:(?:[2-8]\d|9[0-7]|0?[28]|0?9(?=09))(?:\d{2}))$/"/>
                </div>
              </div>
            </div>
            <div class="row third">
              <div class="input-field">
                <button class="btn-search">Search</button>
                <button class="btn-delete" id="delete">Delete</button>
              </div>
            </div>
          </div>
        </div>
      </form>
    </div>

